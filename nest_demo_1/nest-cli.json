{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nest_demo_1/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/nest_demo_1/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/nest_demo_1",
  "projects": {
    "nest_demo_1": {
      "type": "application",
      "root": "apps/nest_demo_1",
      "entryFile": "main",
      "sourceRoot": "apps/nest_demo_1/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest_demo_1/tsconfig.app.json"
      }
    },
    "monorepo_sub_package": {
      "type": "application",
      "root": "apps/monorepo_sub_package",
      "entryFile": "main",
      "sourceRoot": "apps/monorepo_sub_package/src",
      "compilerOptions": {
        "tsConfigPath": "apps/monorepo_sub_package/tsconfig.app.json"
      }
    }
  }
}